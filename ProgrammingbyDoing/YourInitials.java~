public class YourInitials
{
    public static void main( String[] args )
    {

	System.out.println( "For the name O'Tomi Joseph Adeoye..." );
	System.out.println( " OOO	JJJJJ	   A	    	 " );
	System.out.println( "O   O        J       A A            " );
	System.out.println( "O   O        J      A   A           " );
	System.out.println( "O   O        J      AAAAA           " );
	System.out.println( "O   O     J  J      A   A           " );
	System.out.println( "O   O     J  J      A   A           " );
	System.out.println( " OOO        JJ      A   A           " );
    }
}
